{
  "name": "formik-vs-react-final-form",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "cross-env": "^6.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^9.5.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.3",
    "final-form": "^4.18.1",
    "final-form-arrays": "^3.0.0",
    "final-form-focus": "^1.1.2",
    "formik": "^1.5.7",
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "react-markdown": "^4.1.0",
    "react-scripts": "3.0.1",
    "react-spinkit": "^3.0.0",
    "react-syntax-highlighter": "^11.0.2",
    "reselect": "^4.0.0",
    "rmwc": "^5.5.2",
    "yup": "^0.27.0"
  },
  "homepage": "https://hosseintalebi.github.io/formik-vs-react-final-form",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --collectCoverage=true --watchAll=false",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src/test/*.jsx --ext '.js,.jsx' --config .eslintrc",
    "lint:fix": "npm run lint --fix",
    "test:staged": "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
    "ci": "npm lint && cross-env CI=true npm test --coverage",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env CI=true lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run test:staged",
      "git add"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/test/*.{js,jsx}"
    ],
    "coverageReporters":[
       "html",
       "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
